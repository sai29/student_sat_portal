<div class="row">

  <div class="col-md-6 col-md-offset-3">
    
    
      <h3>Edit College</h3>
     <%= simple_form_for @college do |f| %>
  <%= f.input :name %>
  <%= f.input :country,  priority: [ "Singapore" ] %>
  <%= f.input :sat_min_score %>
  <%= f.input :sat_max_score %>
  <%= f.input :tution_fees %> 
  <%= f.button :submit %>
<% end %>
      </div>
</div>

